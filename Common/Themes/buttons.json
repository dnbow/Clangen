{
    "#relation_bar": {
        "colours": {
            "normal_border": "#CEC2A8",
            "filled_bar": "#796045",
            "unfilled_bar": "#CEC2A8"
        },
        "misc": {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "6",
            "border_width": "4",
            "shadow_width": "0"
        }
    },
    "#relation_bar_neg": {
        "prototype": "#relation_bar",
        "colours": {
            "filled_bar": "#E67161"
        }
    },
    "#relation_bar_pos": {
        "prototype": "#relation_bar",
        "colours": {
            "filled_bar": "#8CB250"
        }
    },
    "#relation_bar_dark": {
        "prototype": "#relation_bar",
        "colours": {
            "normal_border": "#2F2918",
            "unfilled_bar": "#2F2918"
        }
    },
    "#relation_bar_neg_dark": {
        "prototype": "#relation_bar_dark",
        "colours": {
            "filled_bar": "#E67161"
        }
    },
    "#relation_bar_pos_dark": {
        "prototype": "#relation_bar_dark",
        "colours": {
            "filled_bar": "#8CB250"
        }
    },
    "#events_cat_profile_button": {
        "colours": {
            "normal_bg": "#655934",
            "hovered_bg": "#1E1809",
            "selected_bg": "#1E1809",
            "normal_text": "#EEE4CD",
            "hovered_text": "#EFE5CE"
        },
        "images": {
            "normal_image": {
                "path": "resources/images/buttons/cat_event_profile.png"
            },
            "hovered_image": {
                "path": "resources/images/buttons/cat_event_profile_hover.png"
            },
            "selected_image": {
                "path": "resources/images/buttons/cat_event_profile.png"
            }
        }
    }
